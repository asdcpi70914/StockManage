<script>
    function init_date(precssid) {
            
        $.each($("."+precssid), function () {
            console.log(precssid);
            new flatpickr($(this), {
                enableTime: true,
                position: "center",
                "locale": "zh_tw",
                dateFormat: "Y-m-d H:i",
                time_24hr: true
            });

           $(this).next().on('click', function () {
                let calendar = flatpickr($(this).parent().find("input"), {
                    enableTime: true,
                    position: "center",
                    "locale": "zh_tw",
                    dateFormat: "Y-m-d H:i",
                    time_24hr: true
                });
                calendar.clear();
            });
            //console.log($(this).html())

        })

        //$.each($("#"+precssid + " .src_clrdate"), function () {
        //    $(this).on('click', function () {
        //        let calendar = flatpickr($(this).parent().find("input"), {});
        //        calendar.clear();
        //    });
        //})
    }

    function init_date_notime(precssid) {
        $.each($("." + precssid), function () {
            console.log(precssid);
            new flatpickr($(this), {
                position: "center",
                "locale": "zh_tw",
                dateFormat: "Y-m-d",
            });

            $(this).next().on('click', function () {
                let calendar = flatpickr($(this).parent().find("input"), {
                    position: "center",
                    "locale": "zh_tw",
                    dateFormat: "Y-m-d",
                });
                calendar.clear();
            });
            //console.log($(this).html())

        })
    }

    function init_month(precssid) {
        $.each($("." + precssid), function () {
            console.log(precssid);
            new flatpickr($(this), {
                "locale": "zh_tw",
                plugins: [
                    new monthSelectPlugin({
                        shorthand: true,
                        dateFormat: "Y-m",
                        altFormat: "M/Y",
                    }),
                ]
            });

            $(this).next().on('click', function () {
                let calendar = flatpickr($(this).parent().find("input"), {
                    "locale": "zh_tw",
                    plugins: [
                        new monthSelectPlugin({
                            shorthand: true,
                            dateFormat: "Y-m",
                            altFormat: "M/Y",
                        }),
                    ]
                });
                calendar.clear();
            });
            //console.log($(this).html())

        })
    }

</script>