@model SRCMenu
@{
    string tagentView = "_self";
  
    if (Model.hasChild)
    {
       

        <li class="nav-item">
            <a class="nav-link menu-link" href="#sidebarLayouts@(Model.pid)" data-bs-toggle="collapse" role="button" target="@tagentView"
           aria-expanded="false" aria-controls="sidebarLayouts@(Model.pid)">
                <i class="@Model.icon"></i> <span data-key="">@Model.name</span>
            </a>
            <div class="collapse in menu-dropdown" id="sidebarLayouts@(Model.pid)">
                <ul class="nav nav-sm flex-column">
                    @foreach (var each in Model.child)
                    {
                        <partial name="_sidebar_detail" for="@each" />
                    }
                </ul>
            </div>
        </li> <!-- end Dashboard Menu -->
    }
    else
    {
        <li class="nav-item">
            <a href="@Model.url" class="nav-link" target="@tagentView"><i class="@Model.icon"></i>@Model.name</a>
        </li>
    }

}